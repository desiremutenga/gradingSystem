  <body>  
  <!-- partial:index.partial.html -->
  <div id="login-form-wrap">
    <h2>Login</h2>
    <section class="home">
        <form id="login-form" action="/login" method="post">
            <p>
            <input type="text" class="username" name="username" placeholder="UserID" required=""><i class="validation"><span></span><span></span></i>
            </p>
            <p>
            <input type="password" class="password" name="password" placeholder="Password" required=""><i class="validation"><span></span><span></span></i>
            </p>
            <p>
            <input type="submit" class="login" value="Login">
            </p>
          </form>
          <div style="color:#fff" class="msg">
            <%= Message %>
          </div>
    </section>
    
    <div id="create-account-wrap">

    </p></div><!--create-account-wrap-->
  </div><!--login-form-wrap-->
  </body>
  </html>